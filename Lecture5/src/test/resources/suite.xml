<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test suite">
    <test name="FactoryTest">

        <classes>
            <class name="FactoryTest"></class>
        </classes>

    </test>

    <test name="TestForOtherMethods">
